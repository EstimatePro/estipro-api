{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "Duration": "00:01:00"
  },
  "Vault": {
    "RootFolder": "estipro"
  },
  "OpenTelemetry": {
    "Exporter.Protocol": "http/protobuf",
    "Exporter.Endpoint": "https://otlp-gateway.grafana.net/otlp",
    "Exporter.Headers.Authorization": "Authorization=Basic"
  }
}